<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/main"
    tools:context=".AdminFragment.DetailTransaksiAdminFragment">

    <TextView
        android:id="@+id/tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_12sdp"
        android:layout_marginTop="@dimen/_12sdp"
        android:layout_marginEnd="@dimen/_12sdp"
        android:layout_marginBottom="@dimen/_12sdp"
        android:fontFamily="@font/popsem"
        android:text="Detail Transaksi"
        android:textColor="@color/black"
        android:textSize="@dimen/_15ssp" />

    <RelativeLayout
        android:layout_below="@id/tv1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/_20sdp"
        android:paddingTop="@dimen/_10sdp"
        android:paddingRight="@dimen/_12sdp"
        android:paddingLeft="@dimen/_12sdp"
        android:background="@drawable/container1">


        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <androidx.core.widget.NestedScrollView
                    android:layout_width="match_parent"
                    android:scrollbars="vertical"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_9sdp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/popmed"
                            android:text="Nama Customer"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:layout_gravity="start"/>

                        <TextView
                            android:id="@+id/etNama"
                            android:layout_width="match_parent"
                            android:padding="@dimen/_12sdp"
                            android:textSize="@dimen/_12sdp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/container_edittext1"/>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_9sdp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/popmed"
                            android:text="Nama Ruangan"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:layout_gravity="start"/>

                        <TextView
                            android:id="@+id/etRoomName"
                            android:layout_width="match_parent"
                            android:padding="@dimen/_12sdp"
                            android:textSize="@dimen/_12sdp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/container_edittext1"/>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_9sdp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/popmed"
                            android:text="Tanggal Sewa"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:layout_gravity="start"/>

                        <TextView
                            android:id="@+id/etTglSewa"
                            android:layout_width="match_parent"
                            android:padding="@dimen/_12sdp"
                            android:textSize="@dimen/_12sdp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/container_edittext1"/>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_9sdp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/popmed"
                            android:text="Tanggal Kembali"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:layout_gravity="start"/>

                        <TextView
                            android:id="@+id/etTglKembali"
                            android:layout_width="match_parent"
                            android:padding="@dimen/_12sdp"
                            android:textSize="@dimen/_12sdp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/container_edittext1"/>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_9sdp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/popmed"
                            android:text="Tanggal Selesai"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:layout_gravity="start"/>

                        <TextView
                            android:id="@+id/etTglSelesai"
                            android:layout_width="match_parent"
                            android:padding="@dimen/_12sdp"
                            android:textSize="@dimen/_12sdp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/container_edittext1"/>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_9sdp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/popmed"
                            android:text="Status Selesai"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:layout_gravity="start"/>

                        <TextView
                            android:id="@+id/etStatusSelesai"
                            android:layout_width="match_parent"
                            android:padding="@dimen/_12sdp"
                            android:textSize="@dimen/_12sdp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/container_edittext1"/>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_9sdp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/popmed"
                            android:text="Status Sewa"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:layout_gravity="start"/>

                        <TextView
                            android:id="@+id/etStatusSewa"
                            android:layout_width="match_parent"
                            android:padding="@dimen/_12sdp"
                            android:textSize="@dimen/_12sdp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/container_edittext1"/>

                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/btnDownloadBukttiPersetujuan"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:icon="@drawable/baseline_download_24"
                            android:layout_marginTop="@dimen/_20sdp"
                            android:text="Download bukti persetujuan"/>

                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/btnAccBuktiPersetujuan"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:backgroundTint="@color/green"
                            app:icon="@drawable/baseline_check_circle_24"

                            android:layout_marginTop="@dimen/_12sdp"
                            android:text="Konfirmasi persetujuan"/>

                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/btnSelesai"
                            android:layout_width="match_parent"
                            app:icon="@drawable/baseline_check_circle_24"
                            android:layout_height="wrap_content"
                            app:backgroundTint="@color/green"
                            android:visibility="gone"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:text="Selesai"/>
                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/btnTolak"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:backgroundTint="@color/red"
                            app:icon="@drawable/baseline_highlight_off_24"
                            android:visibility="gone"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:text="Reject"/>

                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/btnKembali"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:backgroundTint="@color/gray"
                            app:icon="@drawable/baseline_arrow_back_24"
                            android:textColor="@color/black"
                            android:layout_marginTop="@dimen/_12sdp"
                            android:text="Kembali"/>















                    </LinearLayout>
                </androidx.core.widget.NestedScrollView>




            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </RelativeLayout>


</RelativeLayout>